{
  "name": "nomkhonwaan.com",
  "version": "0.3.0",
  "description": "Trust me I'm Petdo",
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack",
    "build:babel": "./node_modules/.bin/babel ./src --ignore dist,node_modules --out-dir ./dist",
    "build:webpack": "./node_modules/.bin/webpack --verbose --colors --display-error-details --production --config ./dist/webpack.config.js",
    "start:server": "node ./dist/index.js",
    "dev:webpack": "./node_modules/.bin/webpack --verbose --colors --display-error-details --progress --config ./src/webpack.config.js",
    "dev:server": "./node_modules/.bin/babel-node ./src/index.js",
    "dev": "cross-env NODE_ENV=development npm run dev:server",
    "start": "cross-env NODE_ENV=production npm run start:server",
    "postinstall": "npm run build",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nomkhonwaan/nomkhonwaan.com.git"
  },
  "author": "Natcha Luang - Aroonchai <me@nomkhonwaan.com>",
  "engines": {
    "node": "5.9.1"
  },
  "license": "MIT",
  "homepage": "https://www.nomkhonwaan.com",
  "dependencies": {
    "bookshelf": "^0.9.4",
    "classnames": "^2.2.5",
    "compression": "^1.6.1",
    "cross-env": "^1.0.7",
    "express": "^4.13.4",
    "helmet": "^2.0.0",
    "knex": "^0.10.0",
    "lodash": "^4.11.2",
    "normalize.css": "^4.1.1",
    "pq": "0.0.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-connect": "^2.1.0",
    "superagent": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.30.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-isomorphic-tools": "^2.2.50"
  }
}
