{
  "name": "nomkhonwaan.com",
  "version": "0.4.0",
  "description": "Trust me I'm Petdo",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": "5.9.1"
  },
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack",
    "build:babel": "babel ./src --ignore dist,node_modules --out-dir ./dist",
    "build:webpack": "webpack --verbose --colors --display-error-details --production --config ./dist/webpack.config.js",
    "dev": "cross-env NODE_ENV=development babel-node src/index.js",
    "dev:debug": "npm run dev -- --debug",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./dist/index.js",
    "test": "cross-env NODE_ENV=test mocha --recursive --reporter spec ./tests/ --compilers js:babel-register",
    "test:debug": "npm test -- --debug-brk",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomkhonwaan/nomkhonwaan.com.git"
  },
  "keywords": [
    "reactjs",
    "redux",
    "universal"
  ],
  "author": "Natcha Luang - Aroonchai <me@nomkhonwaan.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nomkhonwaan/nomkhonwaan.com/issues"
  },
  "homepage": "https://github.com/nomkhonwaan/nomkhonwaan.com#readme",
